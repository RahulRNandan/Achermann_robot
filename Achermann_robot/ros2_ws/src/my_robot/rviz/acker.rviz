Panels:
  - Class: rviz/Displays
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /RobotModel
    Splitter Ratio: 0.5
  - Class: rviz/Views
    Name: Views
    Property Tree Widget:
      Expanded:
        - /Current View
      Splitter Ratio: 0.5
Visualization Manager:
  Displays:
    - Class: rviz/RobotModel
      Enabled: true
      Name: RobotModel
      Robot Description: robot_description
  Global Options:
    Fixed Frame: base_link

